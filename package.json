{
  "name": "@wesgro/react-aria-menubutton-ts",
  "version": "0.0.2-beta.1",
  "description": "A Typescript / modern version of react-aria-menubutton",
  "type": "module",
  "module": "dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.cts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "prepack": "pnpm run build",
    "pack": "pnpm pack",
    "start": "vite",
    "test": "vitest",
    "format": "prettier --write ./src",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "typecheck:package": "attw --pack ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wesgro/react-aria-menubutton-ts.git"
  },
  "author": {
    "name": "Jacob Ketcheson"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wesgro/react-aria-menubutton-ts/issues"
  },
  "homepage": "https://github.com/wesgro/react-aria-menubutton-ts",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "aria",
    "accessibility",
    "menu",
    "dropdown",
    "widget"
  ],
  "dependencies": {
    "focus-group": "^0.3.1"
  },
  "peerDependencies": {
    "@types/react": "^16.14.0 || 17 || 18",
    "@types/react-dom": "^16.14.0 || 17 || 18",
    "react": "^16.14.0 || 17 || 18",
    "react-dom": "^16.14.0 || 17 || 18"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@testing-library/dom": "^10.2.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "happy-dom": "^14.12.3",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.1.0",
    "typescript": "~5.5.2",
    "vite": "^5.3.2",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.5.0"
}
